{% extends 'base.html.twig' %}
{% block content %}

    <table>
        <thead>
        <tr>
            <th>name</th>
            <th>quantity</th>
            <th>description</th>
            <th>price</th>
        </tr>
        </thead>
        <tbody>
    {% for produit in produits %}
        <tr>
            <td>{{ produit.nom}}</td>
            <td>{{ produit.qte }}</td>
            <td>{{ produit.description }}</td>
            <td>{{ produit.prix }}</td>
        </tr>
    {% endfor %}
        </tbody>
    </table>

{% endblock %}