{% trans_default_domain 'FOSUserBundle' %}

<div class="fos_user_user_show register-form">
    <h2>Profile</h2>
    <h4>{{ 'profile.show.username'|trans }}: {{ user.username }}</h4>
    <br>
    <h4>{{ 'profile.show.email'|trans }}: {{ user.email }}</h4>
    <br>
    <a href="{{ path('fos_user_profile_edit') }}" class="primary-btn ">Edit</a>

    <a href="{{ path('fos_user_change_password') }}" class="primary-btn">Change password</a>
    <a href="{{ path('orders') }}" class="primary-btn">orders</a>

</div>
